{
	"manifest_version":2,

	"name":"getting started",
	"description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action":{
    	"default_icon":"icon.png",
    	"default_popup":"popup.html",
    	"default_title":"CLick here"
    },

    "permissions":[
    	"activeTab",
    	"https://ajax.googleapis.com/"
    ]
}